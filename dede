{
    label: { text: 'End Date' },
    dataField: 'requestStatus',
    encodeHtml: false,
    allowFiltering: false,
    allowHeaderFiltering: true,
    customizeText: function (e) {
        var statusMapping = {
            "0": "Draft",
            "1": "Waiting Approval",
            "2": "Rework",
            "3": "Approved",
            "4": "Rejected"
        };

        return statusMapping[String(e.value)] || "Unknown";
    }
}
